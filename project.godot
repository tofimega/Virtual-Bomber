; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="java Bomberman port"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
config/icon="res://icon.svg"

[autoload]

GlobalAccess="*res://scripts/globals/global_access.gd"
Utility="*res://scripts/globals/utility.gd"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[layer_names]

2d_physics/layer_1="edge"
2d_physics/layer_2="wall"
2d_physics/layer_3="crate"
2d_physics/layer_4="bomb"
2d_physics/layer_5="player"
2d_physics/layer_7="enemy"
2d_physics/layer_8="explosion"

[rendering]

textures/canvas_textures/default_texture_filter=0
